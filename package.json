  "scripts": {
    "dev": "next",
    "start": "next start",
    "export": "next export",
    "build": "next build",
    "build-analyze": "cross-env ANALYZE=true next build",
    "check-formatting": "prettier --list-different '**/*.{ts,tsx}'",
